{
  "panes": {
    "quadrants": ["Source", "Console", "TabSet1", "TabSet2"],
    "tabSet1": ["History", "Tutorial", "Presentation"],
    "tabSet2": [
      "Environment",
      "Files",
      "Plots",
      "Connections",
      "Packages",
      "Help",
      "Build",
      "VCS",
      "Viewer",
      "Presentations"
    ],
    "hiddenTabSet": [],
    "console_left_on_top": false,
    "console_right_on_top": true,
    "additional_source_columns": 0
  },
  "font_size_points": 14,
  "editor_theme": "Cobalt",
  "default_project_location": "~",
  "wrap_tab_navigation": false,
  "reuse_sessions_for_project_links": true,
  "show_terminal_tab": false,
  "jobs_tab_visibility": "closed",
  "posix_terminal_shell": "zsh",
  "save_workspace": "never",
  "load_workspace": false,
  "initial_working_directory": "~",
  "always_save_history": false,
  "use_secure_download": false,
  "cran_mirror": {
    "name": "Global (CDN)",
    "host": "RStudio",
    "url": "http://cran.rstudio.com/",
    "secondary": "",
    "country": "us"
  },
  "terminal_track_environment": false,
  "code_completion": "manual",
  "code_completion_other": "manual",
  "enable_snippets": false,
  "show_invisibles": true,
  "scroll_past_end_of_document": true,
  "highlight_r_function_calls": true,
  "rainbow_parentheses": true,
  "code_completion_delay": 0
}
